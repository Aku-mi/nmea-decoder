<div class="row">
    <div class="col col-md-4">
        <div class="card">
            <div class="card card-body">
                <form action="/add" method="POST">
                    <div class="form-group">
                        <input type="text" name="secuence" class="form-control" placeholder="NMEA Sentence" autofocus
                            autocomplete="off" required>
                    </div>
                    <button type="submit" class="btn btn-primary btn-block">Add</button>
                </form>
            </div>
        </div>
        <br>
        <div class="card card-body">
            <blockquote class="blockquote text-center">
                <p class="mb-0">Last Input</p>
            </blockquote>
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th class="text-center">
                            Latitude
                        </th>

                        <th class="text-center">
                            Longitude
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        {{#if empty}}
                        {{#each last}}
                        <td class="text-center">
                            {{this.lat}}
                        </td>
                        <td class="text-center">
                            {{this.lon}}
                        </td>
                        {{/each}}
                        {{/if}}
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

    <script
        src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY&language=en&callback=initMap&libraries=&v=weekly"
        defer>
        </script>
    <script>
        const data = {{{ json last }}}
        const zoom = {{{ json zoom }}}
    </script>
    <script src="/map.js">
    </script>

    <style>
        #map {
            width: 100%;
            height: 500px;
        }
    </style>

    <div class="col col-md-8">
        <div class="card">
            <div class="card-body">
                <div id="map">

                </div>
            </div>
        </div>
    </div>
</div>
